{
    "manifest_version": 3,
    "name": "Browsing History Cleaner",
    "version": "1.0",
    "description": "Clear browsing history for specified websites.",
    "permissions": [
      "history"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  